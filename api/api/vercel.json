{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/admin",
      "dest": "frontend/admin/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    },
    {
      "src": "/",
      "dest": "frontend/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task:/var/task/utils:/var/task/protobufs"
  }
}
